   "source": [
    "#client\n",
    "import socket\n",
    "\n",
    "HOST = '127.0.0.1'\n",
    "PORT = 50000\n",
    "\n",
    "s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)\n",
    "s.connect((HOST, PORT))\n",
    "s.send(\"Hello Server\".encode())\n",
    "data = s.recv(1024)\n",
    "\n",
    "print(data)"
   ]
